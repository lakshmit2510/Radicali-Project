{
  "name": "Parker",
  "version": "1.0.0",
  "private": true,
  "description": "The frontend for Mica",
  "scripts": {
    "start": "webpack-dev-server",
    "dev": "webpack-dashboard -t 'Project' -- webpack-dev-server",
    "clean": "shx rm -rf ./build",
    "build": "npm run clean && cross-env NODE_ENV=\"production\" webpack",
