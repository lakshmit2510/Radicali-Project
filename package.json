    "lodash": "^4.17.4",
    "masonry-layout": "^4.2.0",
    "moment": "^2.23.0",
    "node-sass": "^4.11.0",
    "perfect-scrollbar": "^1.1.0",
    "popper.js": "^1.12.6",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.8",
    "process": "^0.11.10",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dashboard": "^1.0.0",
    "webpack-dev-server": "^2.9.3"
